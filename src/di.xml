<?xml version="1.0" encoding="UTF-8"?>
<dependency-mappings>
	<bean>
		<name>user</name>
		<class>com.yuanhao.model.UserBean</class>
		<property>
			<name>username</name>
		</property>
		<property>
			<name>password</name>
		</property>		
	</bean>
	<bean>
		<name>login</name>
		<class>com.yuanhao.action.LoginAction</class>
		<property>
			<name>userBean</name>
			<ref-class>user</ref-class>
		</property>
	</bean>
	<!-- other beans -->
</dependency-mappings>